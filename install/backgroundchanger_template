 #!/bin/bash
background_changer() {
    for dat in #PICPATH#
    do
        gsettings set org.gnome.desktop.background picture-uri "file:///$dat"
        gsettings set org.gnome.desktop.screensaver picture-uri "file:///$dat"
        echo $dat
        sleep #INTERVAL# 
    done

}

while true
      do
          background_changer
done
